    "char-spinner": "^1.0.1",
    "coveralls": "^2.13.1",
    "github-markdown-css": "^2.8.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^3.0.0",
    "marked": "^0.3.6",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "optional": "^0.1.4",
    "q": "^1.5.0",
    "request": "^2.81.0"
  },
  "dependencies": {
    "swagger-client": "2.1.x"
