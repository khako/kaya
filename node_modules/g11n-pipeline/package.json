  "description": "JavaScript (Node.js, etc) client for Bluemix Globalization Pipeline",
  "main": "index.js",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "test": "mocha",
    "coverage": "nyc mocha && nyc report --reporter=lcov",
